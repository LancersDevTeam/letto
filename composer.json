{
  "name": "lancers/letto",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=5.3",
    "doctrine/instantiator": "1.0.5",
    "guzzlehttp/guzzle": "^3.8",
    "nesbot/carbon": "^1.22",
    "symfony/filesystem": "^v2.8",
    "symfony/event-dispatcher": "^v2.8",
    "symfony/translation": "^v2.8",
    "symfony/yaml": "^v2.8",
    "symfony/config": "^v2.8",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Letto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Letto\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-develop"
    }
  }
}
